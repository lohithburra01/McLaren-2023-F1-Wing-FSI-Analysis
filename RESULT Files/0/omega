FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}

dimensions [0 0 -1 0 0 0 0];
internalField uniform 200;

boundaryField
{
    inlet
    {
        type fixedValue;
        value uniform 200;
    }
    outlet
    {
        type zeroGradient;
    }
    bottom
    {
        type omegaWallFunction;
        value uniform 200;
    }
    top
    {
        type omegaWallFunction;
        value uniform 200;
    }
    sides
    {
        type omegaWallFunction;
        value uniform 200;
    }
    frontwing
    {
        type omegaWallFunction;
        value uniform 200;
    }
}
